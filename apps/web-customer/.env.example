# Customer Web Portal Environment Variables
# Copy this file to .env.local and replace with your actual values

# Google Maps JavaScript API Key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_WEB_GOOGLE_MAPS_API_KEY_HERE

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:3000/api/v1

# Stripe Publishable Key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_YOUR_STRIPE_KEY_HERE

# For production:
# NEXT_PUBLIC_API_URL=https://api.dryjets.com/v1
# NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_YOUR_LIVE_KEY_HERE
